{
  "name": "analog-cafe-server",
  "version": "0.1.22",
  "description": "(Loosely) Collaborative Blogging App by Analog.Cafe (Server)",
  "license": "MPL-2.0",
  "private": true,
  "engines": {
    "node": "10.x",
    "npm": "5.x"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dmitrizzle/Analog.Cafe"
  },
  "dependencies": {
    "@roast-cms/image-froth": "^0.2.3",
    "@sendgrid/mail": "^6.4.0",
    "aws-sdk": "^2.327.0",
    "bluebird": "^3.7.1",
    "body-parser": "^1.19.0",
    "cachegoose": "^8.0.0",
    "chance": "^1.0.10",
    "cloudinary": "^1.16.0",
    "commander": "^2.16.0",
    "compression": "^1.7.1",
    "connect-multiparty": "^2.2.0",
    "connect-redis": "^5.2.0",
    "cors": "^2.8.4",
    "dotenv": "^4.0.0",
    "eslint": "^6.5.1",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "feed": "^1.1.0",
    "image-size": "^0.6.1",
    "isomorphic-unfetch": "^3.0.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.0",
    "mongodb": "^3.4.0",
    "mongoose": "^5.11.12",
    "mongoose-timestamp-date-unix": "^0.6.2",
    "mongoose-ts": "0.1.0",
    "newrelic": "^7.0.2",
    "passport": "^0.4.1",
    "passport-facebook": "^3.0.0",
    "passport-jwt": "^4.0.0",
    "passport-twitter": "^1.0.4",
    "prettier": "^1.18.2",
    "redis": "^3.1.2",
    "redis-delete-pattern": "^0.1.0",
    "request": "^2.88.0",
    "rss": "^1.2.2",
    "shortid": "^2.2.8",
    "sib-api-v3-sdk": "3.0.2",
    "sitemap": "^1.13.0",
    "slugify": "^1.2.1",
    "swagger-jsdoc": "1.9.7",
    "swagger-ui-express": "^2.0.14",
    "url-pattern": "^1.0.3",
    "word-count": "^0.2.2"
  },
  "devDependencies": {
    "husky": "^3.0.9",
    "jest": "^26.6.3",
    "lint-staged": "^9.4.2",
    "nodemon": "^2.0.2",
    "prettier-standard": "^16.1.0",
    "standard": "^14.3.1"
  },
  "scripts": {
    "precommit": "lint-staged",
    "start": "node --harmony index.js",
    "dev": "NODE_ENV=development nodemon --harmony index.js",
    "db:seed": "node --harmony seeds/index.js",
    "db:clean": "node --harmony seeds/clean_db.js",
    "scheduler:publish": "node --harmony workers/publisher/index.js",
    "upload-sitemap-rss": "node --harmony upload_rss_sitemap_cmd.js --sitemap --rss",
    "test": "jest"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/dist/",
      "/examples/",
      "/graphics/"
    ]
  },
  "lint-staged": {
    "{app,models,seeds}/**/*.js": [
      "prettier-standard",
      "git add"
    ]
  }
}
